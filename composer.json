{
    "name": "lara-zeus/uranus",
    "description": "convert any admin panel to filamentphp",
    "keywords": [
        "laravel",
        "lara-zeus",
        "backpackforlaravel",
        "nova",
        "voyager",
        "uranus",
        "filamentphp"
    ],
    "homepage": "https://larazeus.com",
    "support": {
        "issues": "https://github.com/lara-zeus/uranus/issues",
        "source": "https://github.com/lara-zeus/uranus"
    },
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "php coder",
            "email": "info@larazeus.com",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.1",
        "laravel/prompts": "^0.1.0@dev"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "laravel/pint": "^1.0",
        "larastan/larastan": "^2.2",
        "orchestra/testbench": "^8.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "LaraZeus\\Uranus\\": "src"
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "LaraZeus\\Uranus\\UranusServiceProvider"
            ],
            "aliases": {
                "Uranus": "LaraZeus\\Uranus\\UranusFacade"
            }
        }
    }
}
